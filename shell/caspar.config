<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <paths>
    <media-path>L:\\Casparcg\\_media\\</media-path>
    <log-path>L:\\Casparcg\\_log\\</log-path>
    <data-path>L:\\Casparcg\\_data\\</data-path>
    <template-path>L:\\Casparcg\\_templates\\</template-path>
    <template-host>cg.fth.18</template-host>
  </paths>
  <diagnostics>
    <graphs>true</graphs> <!-- [true/false] Displays diagnostics window with real-time graphs. -->
  </diagnostics>
  <channels>
    <channel>
      <!--
        Fully or partially supported video-formats:
          PAL (full support)
          NTSC
          576p2500
          720p2500
          720p5000
          720p5994
          720p6000
          1080p2398
          1080p2400
          1080i5000 (full support)
          1080i5994
          1080i6000
          1080p2500
          1080p2997
          1080p3000
          1080p5000
      -->
      <videomode>1080i5000</videomode>
      <consumers>
        <decklink>
          <device>1</device>                    <!-- [1..] Specifies which device to use. -->
          <embedded-audio>true</embedded-audio> <!-- [true/false] -->
          <low-latency>false</low-latency>      <!-- [true/false] - Experimental feature that reduces frame delay by 3 frames or less. -->
          <external-key>true</external-key>     <!-- [true/false]-->
          <key-only>false</key-only>            <!-- [true/false] - Copies key into fill channels. -->
        </decklink>
        <!--<audio>
        </audio>-->
        <!--<bluefish>
          <device>1</device>
          <embedded-audio>true</embedded-audio>
        </bluefish>-->
      </consumers>
    </channel>
    <channel>
      <videomode>1080i5000</videomode>
      <consumers>
        <audio/>
        <ogl>
          <device>1</device>
          <stretch>uniform</stretch>
          <windowed>true</windowed>
          <output>key_only</output>
        </ogl>
      </consumers>
    </channel>
</channels>
  <controllers>
    <tcp>
      <port>5250</port>
      <protocol>AMCP</protocol>
    </tcp>
  </controllers>
</configuration>